  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.15",
    "@types/jest-matchers": "^20.0.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.3",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.1",
