    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.8.0",
    "jest-environment-enzyme": "^7.0.2",
    "jest-enzyme": "^7.0.2",
    "mocha": "^6.1.4",
    "path": "^0.12.7",
