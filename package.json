{
  "name": "jumbo",
  "version": "0.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "webpack-dev-server --mode development --progress",
    "build": "webpack",
    "test:ui": "jest",
    "test:models": "mocha -r ts-node/register ./src/**/*.test.ts",
